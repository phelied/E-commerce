<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>MatrixTechTips API</title>
            {# Run `composer require symfony/webpack-encore-bundle`
            and uncomment the following Encore helpers to start using Symfony UX #}
        {% block stylesheets %}
            <link href="{{ asset("stylesheets/root.css") }}" rel="stylesheet">
        {% endblock %}
        {% block javascripts %}
            {#{{ encore_entry_script_tags('app') }}#}
        {% endblock %}
    </head>
    <body>
        {% block body %}
            <h1>Matrix<span>TechTips</span>&nbsp;API</h1>
        {% endblock %}
        <div id="infos">
            <div class="columns">
                <div>
                    <h3>Entity</h3>
                </div>
                <div>
                    <h3>Route</h3>
                </div>
                <div>
                    <h3>Method</h3>
                </div>
                <div>
                    <h3>Response ( Success )</h3>
                </div>
                <div>
                    <h3>Response ( Failure )</h3>
                </div>
            </div>
            {% for details in data %}
                {% for row in details %}
                    <div>
                        <div>
                            <p>{{ row.entity }}</p>
                        </div>
                        <div>
                            <p>{{ row.route }}</p>
                        </div>
                        <div>
                            <p>{{ row.method }}</p>
                        </div>
                        <div>
                            <p>{{ row.success }}</p>
                        </div>
                        <div>
                            <p>{{ row.failure }}</p>
                        </div>
                    </div>
                {% endfor %}
            {% endfor %}
        </div>
    </body>
</html>